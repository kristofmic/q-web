<!-- Basic Page Needs
–––––––––––––––––––––––––––––––––––––––––––––––––– -->
<meta charset="utf-8" />
<title><%- title %></title>
<meta name="description" content="Startr" />

<!-- IE
__________________________________________________ -->
<meta http-equiv="x-ua-compatible" content="ie=edge" />

<!-- Mobile Specific Metas
–––––––––––––––––––––––––––––––––––––––––––––––––– -->
<meta
  name="viewport"
  content="minimum-scale=1, initial-scale=1, width=device-width, shrink-to-fit=no, user-scalable=no"
/>

<!-- Social Metas
–––––––––––––––––––––––––––––––––––––––––––––––––– -->
<!-- Twitter Card data -->

<!-- Open Graph data -->

<!-- FONT
–––––––––––––––––––––––––––––––––––––––––––––––––– -->
<link
  href="https://fonts.googleapis.com/css?family=Nunito:400,700,800&display=swap"
  rel="stylesheet"
/>

<%- include('./_styles.ejs', { css: css, env: env })%>

<!-- Favicon
–––––––––––––––––––––––––––––––––––––––––––––––––– -->
<link rel="icon" type="image/png" href="/public/images/favicon.png" />

<!-- Third Party Javascript
–––––––––––––––––––––––––––––––––––––––––––––––––– -->

<% if (!localData || (!localData.user && !localData.config.public &&
!localData.config.error)) { %>
<script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.2/jquery.min.js"></script>
<script
  src="https://apis.google.com/js/client:platform.js?onload=start"
  async
  defer
></script>
<script>
  function start() {
    gapi.load('auth2', function () {
      auth2 = gapi.auth2.init({
        client_id: '<%= googleClientId %>',
      });
    });
  }
</script>
<% } %>
